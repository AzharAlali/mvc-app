<div class="container-fluid">
  <div class=row>
    <div class="col col-md-12">

      <h2>Confirm deletion of student</h2>
      <br>
      <br> ID: <%=  student._id %>
      <br> Given: <%=student.given%>
      <br> Family: <%=student.family%>
      <br> Email: <%=student.email%>
      <br> GPA:<%=student.city %>
      <br> Github: <%=student.state %>
      <br> Website: <%=student.zip %>
      <br>  SectionID: <%=student.country %>
      <hr>
      
      <form method="get" action="/student">
        <input type="submit" value="Return to list" class="btn btn-caution" />
      </form>


      <form method="POST" action="/student/delete/<%= student._id %>">
        <input type="submit" value="Delete this item" class="btn btn-danger pull-right" />
      </form>

    </div>
  </div>
</div>