<br>
<br>
<div class="container-fluid">
  <div class=row>
    <div class="col col-md-12">
      <a href="student/create" class="btn btn-primary new">
        Create a new developer
      </a>

      <h2>Recent entries</h2>
      <p>There are
        <%= students.length %> students.
        <br>
      </p>
      <% for (let i = students.length-1; i >= 0; i--) {   
        student = students[i];   %>

      <div class="panel panel-default">
        <div class="panel-heading"></div>
        <div class="text-muted pull-right"></div>
        <div class="panel-body">

          <br> ID: <%=student._id %>
          <br> Given: <%=student.Given %>
          <br> Family: <%=student.Family %>
          <br> Email: <%=student.Email %>
          <br> GPA: <%=student.GPA %>
          <br> Github: <%=students.Github %>
          <br> Website: <%=students.Website %>
          <br> SectionID: <%=students.SectionID %>

          <hr>
          <a href="/developer/delete/<%= developer._id  %>" class="btn btn-danger"> Delete </a>
          <a href="/developer/details/<%= developer._id %>" class="btn btn-info"> Details </a>
          <a href="/developer/edit/<%= developer._id %>" class="btn btn-primary"> Edit </a>
          <hr>
          <hr>
        </div>
      </div>
      <% } %>

    </div>
  </div>
</div>